{
  "name": "usezodform",
  "version": "0.1.4",
  "description": "A React hook for managing your form via zod",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" ",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "zod",
    "form",
    "react",
    "hook"
  ],
  "author": "Gary Storey <garystorey655@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.2",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "zod": "^3.21.4",
    "react": "^18.2.0"
  }
}
